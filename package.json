{
  "name": "cloud-tutorials",
  "version": "1.1.0",
  "private": false,
  "scripts": {
    "analyze": "npm run build:prod -- --env analyze=true",
    "build": "webpack --mode=production",
    "deploy": "npm-run-all build:prod lint test",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "start": "npm-run-all -p start:*",
    "start:backend": "insights-standalone",
    "start:frontend": "webpack serve",
    "verify": "npm-run-all build lint test"
  },
  "dependencies": {
    "@patternfly/react-core": "^4.101.3",
    "@patternfly/react-table": "^4.23.14",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@redhat-cloud-services/insights-standalone": "^0.1.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.2.0",
    "git-revision-webpack-plugin": "^3.0.6",
    "html-replace-webpack-plugin": "^2.6.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "npm-run-all": "4.1.5",
    "ts-loader": "^8.0.18",
    "tslib": "^1.13.0",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
